<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        package="com.example.pharmacist">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:label="Happy Pills Pharmacy"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:ignore="GoogleAppIndexingWarning"
        tools:replace="android:label">
        <activity android:name=".cart_order"/>
        <activity android:name=".History"/>
        <activity android:name=".stock"/>
        <activity android:name=".profile"/>
        <activity android:name=".index"/>
        <activity android:name=".ViewPatients"/>
        <activity android:name=".Verify" />
        <activity android:name=".DoctorOptions" />
        <activity android:name=".DoctorInfo" />
        <activity android:name=".AdminMainActivity" />
        <activity android:name=".SearchOrdersActivity" />
        <activity android:name=".OrderIndexPageActivity" />
        <activity android:name=".CreateStockItemActivity" />
        <activity android:name=".ListStockActivity" />
        <activity android:name=".SearchStockActivity" />
        <activity android:name=".ListAllOrderActivity" />
        <activity android:name=".ListDoctorsActivity" />
        <activity android:name=".ToDoctorIndexActivity" />
        <activity android:name=".ToStockPageActivity" />
        <activity android:name=".SearchDoctorActivity" />
        <activity android:name=".CreateDoctorActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>